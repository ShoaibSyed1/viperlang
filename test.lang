fn range(start: int, end: int): Range {
    range_inc(start, end, 1)
}

fn range_inc(start: int, end: int, inc: int): Range {
    make Range {
        start: start,
        end: end,
        inc: inc,

        cur: start,
    }
}

class Range {
    let start: int;
    let end: int;
    let inc: int;

    let cur: int;

    fn next(): option int {
        if self.cur >= self.end {
            none(int)
        } else {
            let ret = self.cur;
            self.cur += self.inc;
            some(ret)
        }
    }
}